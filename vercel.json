{
	"version": 2,
	"installCommand": "bun install",
	"builds": [
		{
			"src": "apps/server/src/index.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "apps/server/src/index.ts"
		}
	],
	"functions": {
		"apps/server/src/index.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"ignoreCommand": "git diff --quiet HEAD^ HEAD ./"
}
